{

    "manifest_version": 2,
    "name": "__MSG_ExtensionName__",
    "description": "__MSG_ExtensionDescription__",
    "version": "0.2",

    "background": {
    "scripts": ["background.js"]
    },
  
    "applications": { 
    "gecko": { "strict_min_version": "56.0" }
    },
    
    "icons": {
    "48": "icons/iparcel48.png",
    "96": "icons/iparcel96.png"
    },
  
    "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
    ],
    
    "default_locale": "en"

} 
